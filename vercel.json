{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "swagger-output.json",
                    "src/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api-docs/(.*)",
            "dest": "/api-docs/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}